// STRUCT STUDENT CODE
#include <stdio.h>
#include <string.h>
union student {
    char name[30];
    int roll;
    float marks;
};

int main() {
    union student S1, S2;
    strcpy(S1.name,"John Doe");
    S1.roll = 101;
    S1.marks = 95.5;
    S2 = S1;

    printf("\nName: %s, Roll: %d, Marks: %f\n", S2.name, S2.roll, S2.marks);


    return 0;
}// STRUCT STUDENT CODE
